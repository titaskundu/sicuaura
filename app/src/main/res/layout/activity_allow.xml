<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".allow"
    android:background="@color/hero">


    <TextView
        android:id="@+id/textView11"
        android:layout_width="278dp"
        android:layout_height="24dp"
        android:layout_marginTop="333dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="We need access to your contacts?"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="66dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button2"
        android:layout_width="151dp"
        android:layout_height="49dp"
        android:background="@drawable/roundc2"
        android:fontFamily="@font/poppins_semibold"
        android:text="allow"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="385dp"
        android:drawableRight="@drawable/baseline_check_24"
        android:paddingRight="25dp"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button3"
        android:layout_width="151dp"
        android:layout_height="49dp"
        android:background="@drawable/round2c2"
        android:text="Deny"
        tools:layout_editor_absoluteX="208dp"
        tools:layout_editor_absoluteY="385dp"
        android:fontFamily="@font/poppins_semibold"
        android:textColor="#95929D"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>