<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/hero"
    tools:context=".LoginLaunch">


    <TextView
        android:id="@+id/textView1"
        android:layout_width="78dp"
        android:layout_height="39dp"
        android:fontFamily="@font/poppins_semibold"
        android:lineHeight="36dp"
        android:text="Safe"
        android:textAlignment="center"
        android:textColor="@color/cyan"
        android:textFontWeight="600"
        android:textSize="24dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="453dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="78dp"
        android:layout_height="39dp"
        android:layout_marginStart="60dp"
        android:fontFamily="@font/poppins_semibold"
        android:lineHeight="36dp"
        android:text="Feel"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="453dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="180dp"
        android:layout_height="285.5dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="300dp"
        android:maxWidth="180px"
        android:maxHeight="285.55px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/grp"
        tools:ignore="MissingConstraints"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="154dp"
        android:layout_height="39dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/poppins_semibold"
        android:lineHeight="36dp"
        android:text="Everywhere"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="453dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="99dp"
        android:layout_height="39dp"
        android:layout_marginStart="120dp"
        android:fontFamily="@font/poppins_light"
        android:lineHeight="36dp"
        android:text=" #Safe-"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="501dp" />

    <TextView
        android:layout_width="53dp"
        android:layout_height="39dp"
        android:fontFamily="@font/poppins_light"
        android:lineHeight="36dp"
        android:text="T"
        android:textAlignment="center"
        android:textColor="@color/cyan"
        android:textFontWeight="600"
        android:textSize="20dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="184dp"
        tools:layout_editor_absoluteY="501dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="78dp"
        android:layout_height="39dp"
        android:layout_marginStart="200dp"
        android:fontFamily="@font/poppins_light"
        android:lineHeight="36dp"
        android:text="-Fast"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textFontWeight="600"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="501dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="300dp"
        android:layout_height="50dp"

        android:layout_marginTop="92dp"
        android:background="@drawable/rounded_corner"
        android:fontFamily="@font/poppins_semibold"

        android:text="LOGIN"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:layout_editor_absoluteX="55dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:fontFamily="@font/poppins_light"
        android:text="New User ?"
        android:textColor="#96939E"
        android:textSize="14dp"
        app:lineHeight="21dp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="696dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="21dp"
        android:fontFamily="@font/poppins_light"
        android:text="Register Now"
        android:textColor="@color/bluecyan"
        android:textSize="14dp"
        app:lineHeight="21dp"
        tools:layout_editor_absoluteX="213dp"
        tools:layout_editor_absoluteY="696dp" />


</androidx.constraintlayout.widget.ConstraintLayout>